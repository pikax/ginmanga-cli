{
  "name": "ginmanga-cli",
  "version": "0.3.0",
  "main": "lib/index.js",
  "repository": "https://github.com/pikax/ginmanga-cli",
  "author": "pikax",
  "license": "MIT",
  "bin": {
    "ginmanga": "bin/ginmanga"
  },
  "keywords": [
    "manga",
    "downloader",
    "crawler",
    "manga-downloader",
    "gin-manga",
    "manga-fox",
    "mangafox",
    "mangapanda",
    "mangahere"
  ],
  "scripts": {
    "build": "babel -d lib/ src/",
    "nyc": "nyc node_modules/mocha/bin/_mocha test/**/*.spec.js",
    "lint": "eslint ./src/*/**.js ./test/*/**.spec.js  --quiet; exit 0",
    "test:watch": "mocha test/**/*.spec.js --colors --watch",
    "mocha": "mocha test/**/*.spec.js --colors",
    "test": "cross-env DEBUG=\"gin-downloader:*,-gin-downloader:*:verbose\" istanbul cover node_modules/mocha/bin/_mocha  test/**/*.spec.js -- -R spec",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha ./test/mangapanda/index.spec.js"
  },
  "bugs": {
    "url": "https://github.com/pikax/ginmanga-node/issues"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "homepage": "https://github.com/pikax/ginmanga-cli#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.12.0",
    "cross-env": "^3.2.4",
    "eslint": "^3.17.1",
    "istanbul": "v1",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2"
  },
  "dependencies": {
    "@types/commander": "^2.3.31",
    "@types/fs-extra": "^2.0.0",
    "@types/progress": "^1.1.28",
    "bluebird": "^3.4.7",
    "commander": "^2.9.0",
    "debug": "^2.6.3",
    "fs-extra": "^2.0.0",
    "gin-downloader": "^1.0.0",
    "hyperquest": "^2.1.2",
    "progress": "^2.0.0"
  }
}
